void updateGoblin(GOBLIN *g) {

 if (dirtPatches[g->locIndex].remaining <= 0 && g->health > 0) {
  if (g->isQueen) {
   playSoundB(QueenSFX, 44641, 11025, 0);
   queenActivated = 1;
  }
  g->isActive = 1;
 }

 if (g->isActive) {

  if (g->row == 0 || g->row == 160 -g->height || g->col == 0 || g->col == 240 -g->width) {
   if (curCollect > 0) {
    curCollect--;
    playSoundB(TheftSFX, 1588, 11025, 0);

    for (int i = coinCount-1; i >= 0; i--) {
     if (collectedCoins[i].isActive) {
      collectedCoins[i].isActive = 0;
      break;
     }
    }
   }
   g->health = 0;
   g->isActive = 0;
  }


  if (dwarf.isSwinging && collision(g->row, g->col, g->height, g->width,
   dwarf.row, dwarf.col, dwarf.height, dwarf.width)) {
   if (g->health == 0) {
    playSoundB(DeathSFX, 8320, 11025, 0);
    g->isActive = 0;
   }
   g->health--;
  }



  if (g->locIndex && 1 == 0)
   g->row += g->rVel;
  else
   g->col += g->cVel;
 }
}

void animateGoblin(GOBLIN *g) {

 g->prevAniState = g->aniState;


 if (g->aniCounter % 20 == 0)
  g->curFrame = (g->curFrame + 1) % g->numFrames;

 if (g->locIndex && 1 == 0) {
  if (g->rVel < 0)
   g->aniState = GOBLIN_BACK;
  else
   g->aniState = GOBLIN_FRONT;
 } else {
  if (g->cVel < 0)
   g->aniState = GOBLIN_LEFT;
  else
   g->aniState = GOBLIN_RIGHT;
 }

 g->aniCounter++;
}

void drawGoblin(GOBLIN *g) {
 if (g->isActive) {
  shadowOAM[g->oamIndex].attr0 = (0xFF & g->row) | (0<<13) | (0<<14);
  shadowOAM[g->oamIndex].attr1 = (0x1FF & g->col) | (1<<14);
  shadowOAM[g->oamIndex].attr2 = ((g->curFrame*2)*32+(g->aniState*2)) | ((0)<<12);
 } else {
  shadowOAM[g->oamIndex].attr0 = (2<<8);
 }
}



