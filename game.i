void updateCoin(COIN *c) {

 if (dirtPatches[c->locIndex].remaining <= 0 && c->isCollected == 0) {
  playSoundB(CoinSFX, 4973, 11025, 0);
  c->isActive = 1;
  c->isCollected = 1;
  coinsRemaining--;
  curCollect++;
 }


 if (c->isActive) {
  c->row--;


  if (c->row == 0) {
   c->isActive = 0;


   for (int i = 0; i < coinCount; i++)
    if (!collectedCoins[i].isActive) {
     collectedCoins[i].isActive = 1;
     break;
    }
  }
 }
}

void drawCoin(COIN *c) {
 if (c->isActive && !c->isStolen) {
  shadowOAM[c->oamIndex].attr0 = (0xFF & c->row) | (0<<13) | (0<<14);
  shadowOAM[c->oamIndex].attr1 = (0x1FF & c->col) | (1<<14);
