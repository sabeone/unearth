 for (int i = 0; i < goblinCount; i++) {
  goblins[i].oamIndex = curOamIndex;
  curOamIndex++;
  goblins[i].height = 16;
  goblins[i].width = 16 -1;


  goblins[i].locIndex = curLocationIndex;
  goblins[i].row = locations[curLocationIndex].validRow;
