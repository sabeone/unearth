void initDwarf() {
 dwarf.oamIndex = 0;
 dwarf.height = 16;
 dwarf.width = 16;
 dwarf.row = 160/2 - dwarf.height/2;
 dwarf.col = 240/2 - dwarf.width/2;
 dwarf.rVel = 2;
 dwarf.cVel = 2;
 dwarf.isSwinging = 0;
 dwarf.aniCounter = 0;
 dwarf.aniState = DWARF_FRONT;
 dwarf.prevAniState = dwarf.aniState;
 dwarf.curFrame = 0;
 dwarf.numFrames = 3;
}

void updateDwarf() {

 if ((!(~(oldButtons)&((1<<1))) && (~buttons & ((1<<1))))) {
  playSoundB(CheatSFX, 14928, 11025, 0);
  cheatEnable = !cheatEnable;
  goblins[0].isQueen = !goblins[0].isQueen;
 }


 if ((!(~(oldButtons)&((1<<0))) && (~buttons & ((1<<0))))) {
  playSoundB(AxeSFX, 5888, 11025, 0);
  dwarf.isSwinging = 1;
 }
 else
  dwarf.isSwinging = 0;


 if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<6))) && (dwarf.row-dwarf.rVel > 0))
  dwarf.row -= dwarf.rVel;
 if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<7))) && (dwarf.row+dwarf.rVel < 160 -dwarf.height))
  dwarf.row += dwarf.rVel;
 if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<5))) && (dwarf.col-dwarf.cVel > 0))
  dwarf.col -= dwarf.cVel;
 if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<4))) && (dwarf.col+dwarf.cVel < 240 -dwarf.width))
  dwarf.col += dwarf.cVel;
}

void animateDwarf() {

 dwarf.prevAniState = dwarf.aniState;
 dwarf.aniState = DWARF_IDLE;


 if (dwarf.aniCounter % 20 == 0)
  dwarf.curFrame = (dwarf.curFrame + 1) % dwarf.numFrames;


 if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<6))))
   dwarf.aniState = DWARF_BACK;
 if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<7))))
   dwarf.aniState = DWARF_FRONT;
 if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<5))))
   dwarf.aniState = DWARF_LEFT;
 if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<4))))
   dwarf.aniState = DWARF_RIGHT;


 if (dwarf.aniState == DWARF_IDLE) {
  dwarf.curFrame = 0;
  dwarf.aniCounter = 0;
  dwarf.aniState = dwarf.prevAniState;
 } else {
  dwarf.aniCounter++;
 }
}

void drawDwarf() {
 shadowOAM[dwarf.oamIndex].attr0 = (0xFF & dwarf.row) | (0<<13) | (0<<14);
 shadowOAM[dwarf.oamIndex].attr1 = (0x1FF & dwarf.col) | (1<<14);
 shadowOAM[dwarf.oamIndex].attr2 = ((dwarf.curFrame*2)*32+(dwarf.aniState*2)) | ((0)<<12);
}




